import random 
import math 
import matplotlib.pyplot as plt
import statistics as stats
import collections

def middle_square (seed,n):
    semillas=[]
    numeros=[]
    t=1
    #print(seed)
    for i in range(1, n):   
        s=[]
        x = seed * seed
        #print(x)
        numeros.append(x)
        #print (numeros)
        dig_x= len(str(numeros[t-1]))
        #print(dig_x)
        if dig_x != 8:
            new_seed = str(x).zfill(8)
        x_list=list(new_seed)
        #print(x_list)
        for i in range(2,6):
            s+=x_list[i]    
        #print(s)
        seed = int(''.join(s))
        semillas.append(seed) 
    print(numeros,semillas)

seed= int(input("Ingrese una semillad de 4 numeros: "))
n= int(input("Ingrese cantidad de repeticiones: "))
middle_square(seed, n) 


